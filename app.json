{
  "expo": {
    "name": "Bombastic",
    "slug": "poc-multi-player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "localhost",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.bombastic.pocmultiplayer",
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bombastic.pocmultiplayer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "react-native-nfc-manager",
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Brice-BoldSemiExpanded.ttf",
            "./assets/fonts/Brice-LightSemiExpanded.ttf",
            "./assets/fonts/Balgin Black Sm Condensed.ttf",
            "./assets/fonts/balgintext-regularnarrow.otf",
            "./assets/fonts/balgintext-boldnarrow.otf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0d0cf37a-e273-4b1b-b676-f45e2f925de8"
      }
    }
  }
}
